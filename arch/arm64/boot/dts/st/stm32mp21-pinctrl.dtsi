		};
		pins3 {
			pinmux = <STM32_PINMUX('H', 9, AF6)>; /* USART6_RX */
			bias-pull-up;
		};
	};

	usart6_sleep_pins_a: usart6-sleep-0 {
		pins {
			pinmux = <STM32_PINMUX('F', 13, ANALOG)>, /* USART6_TX */
				 <STM32_PINMUX('C', 3, ANALOG)>,  /* USART6_RTS */
